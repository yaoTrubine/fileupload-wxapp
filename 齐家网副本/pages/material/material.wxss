/* material.wxss */

.vendor{
  display: flex;
  flex-direction: row;
  justify-content: center;
  background: grey;
  flex-wrap: wrap;
}
.vendor image{
  width: 200rpx;
  height: 200rpx;
  padding-left: 10rpx;
}

.vendor text{
  display: block;
  color: #fff;
  font-weight: bolder;
  text-align: center;
  padding: 10rpx 0;
}
.vendorObj{
  display: flex;
  flex-direction: row;
  width: 600rpx;
}