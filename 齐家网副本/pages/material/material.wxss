/* material.wxss */

.vendor{
  display: flex;
  flex-direction: row;
  justify-content: center;
  background: grey;
  flex-wrap: wrap;
}
.vendor image{
  width: 200rpx;
  height: 200rpx;
  padding: 0 15rpx 15rpx 0;
}

.vendor text{
  display: block;
  color: #fff;
  font-weight: bolder;
  text-align: center;
  padding: 10rpx 0;
}
.vendorObj{
  padding: 10rpx 0;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
}